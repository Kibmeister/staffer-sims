# Staging Environment Configuration
ENVIRONMENT=staging
DEBUG=false
LOG_LEVEL=INFO

# API Provider (can be overridden in .env)
API_PROVIDER=openrouter

# Staging-specific settings
MAX_TURNS=15
REQUEST_TIMEOUT=120
RETRY_ATTEMPTS=3

# Staging URLs (staging services)
SUT_URL=https://staging-sut.yourcompany.com/sut/chat
PROXY_URL=https://openrouter.ai/api/v1/chat/completions
LANGFUSE_HOST=https://cloud.langfuse.com
